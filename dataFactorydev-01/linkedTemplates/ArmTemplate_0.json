				},
				"schema": {}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_blob_dev_01')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/ds_read_Parquet_01')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_blob_dev_01",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "Parquet",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": "Input_demo_01.parquet",
						"container": "devcontainer02"
					},
					"compressionCodec": "snappy"
				},
				"schema": [
					{
						"name": "EmpId",
						"type": "UTF8"
					},
					{
						"name": "Name",
						"type": "UTF8"
					},
					{
						"name": "Primaryskill",
						"type": "UTF8"
					},
					{
						"name": "Location",
						"type": "UTF8"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_blob_dev_01')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/ls_parquet_output')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_blob_dev_01",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
